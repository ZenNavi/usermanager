<!-- ajax modals -->
<div class="modal" id="ajaxLoading" title="Please Wait">
	<img src="static/images/loading.gif" />
	<p></p>
</div>

<div class="modal" id="ajaxError" title="Ajax Request Failed">
	<span class="icon ui-icon  ui-icon-closethick"></span>
	Sorry, your request was not fulfilled. Maybe server is down.
</div>

<div class="modal" id="processError" title="">
	<span class="icon ui-icon  ui-icon-circle-close"></span>
	<b>Server said: </b>
	<span class="info"></span>
</div>

<!--  delete confirm dialog -->
<div class="modal" id="confirmDeletion" title="Delete User">
 	<span class="icon ui-icon ui-icon-alert"></span>
 	User "<span class="name"></span>" will be permanently deleted. <b>Are you sure?</b>
</div>
